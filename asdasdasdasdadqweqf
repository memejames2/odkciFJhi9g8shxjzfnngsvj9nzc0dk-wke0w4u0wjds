    HELL["UIStroke"]["ApplyStrokeMode"] = Enum.ApplyStrokeMode.Border
    HELL["UIStroke"]["Color"] = Color3.fromRGB(255, 255, 255)
 
     -- Button click handler
     HELL["Button2"].MouseButton1Click:Connect(function()
         pcall(call)
     end)
 end
 
 -- Get the key function
 function HELL:GetKey(Text, call)
 HELL["Button1"]["Parent"] = HELL["MainFrame"]
 HELL["Button1"]["Name"] = 'Button1'
 HELL["Button1"]["FontFace"] = Font.new("rbxasset://fonts/families/GothamSSm.json", Enum.FontWeight.Bold, Enum.FontStyle.Normal)
 HELL["Button1"]["FontSize"] = Enum.FontSize.Size14
 HELL["Button1"]["Text"] = Text
 HELL["Button1"]["TextColor3"] = Color3.fromRGB(255, 255, 255)
 HELL["Button1"]["TextSize"] = 14
 HELL["Button1"]["AnchorPoint"] = Vector2.new(0.500000, 0.500000)
 HELL["Button1"]["BackgroundColor3"] = Color3.fromRGB(0, 0, 0)
 HELL["Button1"]["BackgroundTransparency"] = 0.5
 HELL["Button1"]["BorderColor3"] = Color3.fromRGB(0, 0, 0)
 HELL["Button1"]["BorderSizePixel"] = 0
 HELL["Button1"]["Position"] = UDim2.new(0.250000, 0.000000, 0.650000, 0.000000)
 HELL["Button1"]["Size"] = UDim2.new(0.447547, 0.000000, 0.155089, 0.000000)
 HELL["Button1"]["ZIndex"] = 3
 
 HELL["UICorner_5"]["Parent"] = HELL["Button1"]
 HELL["UICorner_5"]["Name"] = 'UICorner'
 HELL["UICorner_5"]["CornerRadius"] = UDim.new(0.000000, 2.000000)
 
 HELL["DropShadow_3"]["Parent"] = HELL["Button1"]
 HELL["DropShadow_3"]["Name"] = 'DropShadow'
 HELL["DropShadow_3"]["Image"] = 'rbxassetid://6015897843'
 HELL["DropShadow_3"]["ImageColor3"] = Color3.fromRGB(0, 0, 0)
 HELL["DropShadow_3"]["ImageTransparency"] = 0.6000000238418579
 HELL["DropShadow_3"]["ScaleType"] = Enum.ScaleType.Slice
 HELL["DropShadow_3"]["SliceCenter"] = Rect.new(49, 49, 450, 450)
 HELL["DropShadow_3"]["AnchorPoint"] = Vector2.new(0.500000, 0.500000)
 HELL["DropShadow_3"]["BackgroundTransparency"] = 1
 HELL["DropShadow_3"]["BorderSizePixel"] = 0
 HELL["DropShadow_3"]["Position"] = UDim2.new(0.500000, 0.000000, 0.500000, 0.000000)
 HELL["DropShadow_3"]["Size"] = UDim2.new(1.000000, 37.000000, 1.000000, 37.000000)
 
 HELL["UIStroke_3"]["Parent"] = HELL["Button1"]
 HELL["UIStroke_3"]["Name"] = 'UIStroke'
 HELL["UIStroke_3"]["ApplyStrokeMode"] = Enum.ApplyStrokeMode.Border
 HELL["UIStroke_3"]["Color"] = Color3.fromRGB(255, 255, 255)
 
 TweenServ:Create(HELL["UIStroke_3"],TweenInfo.new(1,Enum.EasingStyle.Quint,Enum.EasingDirection.InOut),{
   Transparency = 0.900
 }):Play();
 
     -- Button click handler
     HELL["Button2"].MouseButton1Click:Connect(function()
         pcall(call)
     end)
 end
    
    function Close()
        TweenServ:Create(HELL["MainDropShadow"], TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut), {
            ImageTransparency = 1
        }):Play()
      
        BlueEffect.Destroy()
      
        for _, v in ipairs(cose) do
            game:GetService('RunService'):UnbindFromRenderStep(v)
        end
      
        TweenServ:Create(HELL["MainFrame"], TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut), {
            Size = UDim2.new(0.8, 0, 0.8, 0)
        }):Play()
        
        task.delay(1, function()
            TweenServ:Create(HELL["MainFrame"], TweenInfo.new(1, Enum.EasingStyle.Quint, Enum.EasingDirection.InOut), {
                Position = UDim2.new(0.5, 0, 5.5, 0),
                Size = UDim2.new(0.8, 0, 0.8, 0)
            }):Play()
      
            task.delay(1.2, function()
                HELL["ok"]:Destroy()
            end)
        end)
    end
    return HELL
end

return Library

   Transparency = 0.900
 }):Play();
     -- FocusLost event listener for capturing input text
     HELL["TextBox"].FocusLost:Connect(function(enterPressed)
         if callback then
             callback(HELL["TextBox"].Text)
         end
     end)
 end
 
 -- Button to check the key
 function HELL:CheckKey(Text, call)
